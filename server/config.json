{
  "runClustered": true,
  "legacyExplorer": false,
  "remoting": {
    "context": {
      "enableHttpContext": false
    },
    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": {
      "origin": true,
      "credentials": true
    },
    "errorHandler": {
      "disableStackTrace": false
    }
  },
  "explorer": {
    "realm": "{api}.explorer",
    "user": "{api}",
    "auth": "m0nk3yb!z"
  }
}
